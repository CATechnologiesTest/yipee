{
  "name": "yipee",
  "version": "2.0.229",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rm -rf ./dist",
    "clean-all": "rm -rf ./dist && rm -rf ./node_modules",
    "start": "ng serve --port 8080 --proxy-config proxy.config.js",
    "build": "ng build",
    "coverage": "ng test --single-run --code-coverage",
    "build-prod-version": "npm version patch && node ./replace.build.js && npm run build-prod",
    "build-prod": "npm run clean && ng build --target=production --env=prod",
    "test-prod": "npm run clean && npm run lint && npm run test && ng build --target=production --env=prod",
    "build-beta": "npm run clean && npm run lint && npm run test && ng build --target=production --env=beta --base-href=/beta/",
    "test": "ng test --watch=false --sourcemaps=false --tags $tags",
    "test-watch": "ng test --sourcemaps=false --tags $tags",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/common": "5.2.0",
