    "@webcomponents/custom-elements": "1.0.0",
    "angular2-draggable": "1.3.1",
    "angular2-moment": "1.8.0",
    "backbone": "1.3.3",
    "bootstrap": "4.0.0-alpha.5",
    "core-js": "2.5.3",
    "dagre": "0.7.4",
    "file-saver": "1.3.3",
    "graphlib": "2.1.1",
    "jointjs": "^2.1.4",
